{
  "id": "runstr",
  "name": "RUNSTR - Run Tracker",
  "shortName": "RUNSTR",
  "version": "0.01",
  "description": "A run tracking app for Bangle.js. Track your runs with GPS, view statistics, and sync with the RUNSTR phone app (coming soon).",
  "icon": "app.png",
  "tags": "health,outdoors,gps,tool",
  "type": "app",
  "supports": ["BANGLEJS2"],
  "readme": "README.md",
  "interface": "interface.html",
  "storage": [
    {"name":"runstr.app.js","url":"app.js"},
    {"name":"runstr.img","url":"app-icon.js","evaluate":true},
    {"name":"runstr.settings.js","url":"settings.js"}
  ],
  "data": [
    {"name":"runstr.json"},
    {"wildcard":"runstr.run.*"}
  ]
} 